<% title "Assets" %>

<table>
  <tr>
    <th>Type</th>
    <th>Name</th>
    <th>Url</th>
    <th>Attachment File Name</th>
    <th>Attachment Content Type</th>
    <th>Attachment File Size</th>
    <th>Attachment Updated At</th>
    <th>State</th>
    <th>Assetable</th>
  </tr>
  <% for asset in @assets %>
    <tr>
      <td><%=h asset.type %></td>
      <td><%=h asset.name %></td>
      <td><%=h asset.url %></td>
      <td><%=h asset.attachment_file_name %></td>
      <td><%=h asset.attachment_content_type %></td>
      <td><%=h asset.attachment_file_size %></td>
      <td><%=h asset.attachment_updated_at %></td>
      <td><%=h asset.state %></td>
      <td><%=h asset.assetable %></td>
      <td><%= link_to "Show", asset %></td>
      <td><%= link_to "Edit", edit_asset_path(asset) %></td>
      <td><%= link_to "Destroy", asset, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Asset", new_asset_path %></p>
